<catalog>
  <widget>
    <name>widget-new-transfer</name>
    <contextItemName>[BBHOST]</contextItemName>
    <extendedItemName>widget-launchpad-base</extendedItemName>
    <properties>
      <property label="Icon" name="icon" viewHint="text-input,admin,designModeOnly">
        <value type="string">new-transfer</value>
      </property>
      <property label="Title" name="title" viewHint="text-input,admin,designModeOnly">
        <value type="string">New Transfer</value>
      </property>
      <property name="thumbnailUrl">
        <value type="string">$(contextRoot)/static/launchpad/banking/widgets/new-transfer/icon.png</value>
      </property>
      <property name="src">
        <value type="string">$(contextRoot)/static/launchpad/banking/widgets/new-transfer/index.html</value>
      </property>
      <property label="Top balance" name="accountsTopBalance" viewHint="select-one,admin,designModeOnly">
        <value type="string">current</value>
      </property>
      <property label="Accounts Data Source" name="accountsDataSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/debit-accounts</value>
      </property>
      <property label="Payment Orders Data Source" name="paymentOrdersDataSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/payment-orders/:accountIds/:paymentId/:action</value>
      </property>
      <property label="Contact List Data Source" name="contactListDataSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/counterparties</value>
      </property>
      <property label="Contact Data Source" name="contactDataSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/party-data-management/counter-party/$(contactId)</value>
      </property>
      <property label="Contact Details Data Source" name="contactDetailsDataSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/party-data-management/counter-party/$(contactId)/details</value>
      </property>
      <property label="IBAN data source" name="ibanDataSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/iban</value>
      </property>
      <property label="Enable Search on IBAN country select" name="enableIbanCountrySearch" viewHint="checkbox,admin,designModeOnly">
        <value type="boolean">true</value>
      </property>
      <property label="Currency exchange rate data source" name="currencyListEndpoint" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/currency-rates</value>
      </property>
      <property label="Default currency data source" name="defaultCurrencyEndpoint" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/currency-rates/default</value>
      </property>
      <property label="Disable currency select" name="disableCurrencySelection" viewHint="checkbox,admin,designModeOnly">
        <value type="boolean">false</value>
      </property>
      <property label="Content Template" name="templateUrl" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(contextRoot)/static/launchpad/html/new-transfer/iceContent.html</value>
      </property>
      <property label="Force initial account selection" name="forceAccountSelection" viewHint="checkbox,admin,designModeOnly">
        <value type="boolean">false</value>
      </property>
      <property label="Enrollment Status Endpoint for P2P Service" name="p2pEnrollmentEndpoint" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/p2p-enrollment</value>
      </property>
      <property label="Autosave Contacts" name="autosaveContactsPreference" viewHint="checkbox,admin,designModeOnly">
        <value type="boolean">false</value>
      </property>
      <property label="Messages Source" name="messageSrc" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(contextRoot)/static/launchpad/banking/widgets/new-transfer/data/messages.json</value>
      </property>
      <property label="Locale" name="locale" viewHint="text-input,admin,designModeOnly">
        <value type="string">en</value>
      </property>
    </properties>
    <tags>
      <tag type="regular">transactions</tag>
      <tag type="section">banking</tag>
      <tag type="behavior">new-transfer</tag>
    </tags>
    <securityProfile>ADMIN</securityProfile>
  </widget>
</catalog>