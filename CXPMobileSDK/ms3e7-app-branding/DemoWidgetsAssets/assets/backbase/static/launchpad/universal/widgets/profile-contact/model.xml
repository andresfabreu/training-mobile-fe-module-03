<catalog>
  <widget>
    <name>widget-profile-contact</name>
    <contextItemName>[BBHOST]</contextItemName>
    <extendedItemName>widget-launchpad-base</extendedItemName>
    <properties>
      <property name="src">
        <value type="string">$(contextRoot)/static/launchpad/universal/widgets/profile-contact/index.html</value>
      </property>
      <property name="title" label="Title" viewHint="text-input,admin,designModeOnly">
        <value type="string">Profile - Contact</value>
      </property>
      <property name="thumbnailUrl">
        <value type="string">$(contextRoot)/static/launchpad/universal/widgets/profile-contact/icon.png</value>
      </property>
      <property name="saveUrl" label="Contact Store URL" viewHint="text-input,admin,designModeOnly">
        <value type="string">$(servicesPath)/services/rest/v1/party-data-management/locations</value>
      </property>
    </properties>
    <tags>
      <tag type="regular">profile</tag>
      <tag type="section">banking</tag>
    </tags>
    <securityProfile>ADMIN</securityProfile>
  </widget>
</catalog>